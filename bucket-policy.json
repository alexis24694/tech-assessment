    "Version": "2012-10-17",
    "Id": "IPRestrictions",
    "Statement": [
        {
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::tech-assessment-bucket-frontend/*",
            "Condition": {
                "IpAddress": {
                    "aws:SourceIp": "179.6.14.111"
